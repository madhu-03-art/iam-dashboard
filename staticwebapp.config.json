{
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.html"
    },
    {
      "route": "/index.html",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/auth/login/aad",
      "statusCode": 200
    },
    {
      "route": "/.auth/me",
      "statusCode": 200
    },
    {
      "route": "/auth/*",
      "rewrite": "/index.html"
    },
    {
      "route": "/.auth/login/aad/callback",
      "rewrite": "/index.html"
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/auth/login/aad",
      "statusCode": 302
    }
  }
}
